{
  "name": "linkdash",
  "version": "0.0.1",
  "description": "A links dashboard",
  "bin": {
    "linkdash": "./lib/bin.js"
  },
  "types": "./lib/index.d.ts",
  "main": "./lib/index.js",
  "scripts": {
    "build:chmodbin": "chmod +x lib/bin.js",
    "build:lib": "rm -rf lib && tsc -p tsconfig.lib.json && chmod +x ./lib/bin.js",
    "build:template": "webpack --config webpack.prod.js",
    "build": "yarn clean && yarn build:template && yarn build:lib && yarn build:chmodbin",
    "clean": "rm -rf assets; rm -rf lib;",
    "dev:server": "json-server --watch src/fake.json --port 1337",
    "dev": " webpack-dev-server --config webpack.common.js",
    "update": "ncu -u",
    "prepublishOnly": "yarn test && yarn build",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/igimanaloto/linkdash"
  },
  "author": "Igi Manaloto",
  "license": "MIT",
  "homepage": "https://github.com/igimanaloto/linkdash#readme",
  "dependencies": {
    "command-line-args": "^5.1.1",
    "command-line-usage": "^6.1.0",
    "open": "^7.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@types/classnames": "^2.2.10",
    "@types/command-line-args": "^5.0.0",
    "@types/command-line-usage": "^5.0.1",
    "@types/jest": "^25.2.1",
    "@types/match-sorter": "^4.0.0",
    "@types/qs": "^6.9.1",
    "@types/react": "^16.9.32",
    "@types/react-dom": "^16.9.6",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "axios": "^0.19.2",
    "babel-loader": "^8.1.0",
    "change-case": "^4.1.1",
    "classnames": "^2.2.6",
    "copy-webpack-plugin": "^5.1.1",
    "core-js": "^3.6.4",
    "css-loader": "^3.5.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.0.4",
    "jest": "^25.2.7",
    "json-server": "^0.16.1",
    "match-sorter": "^4.1.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "npm-check-updates": "^4.1.2",
    "prettier": "^2.0.4",
    "qs": "^6.9.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.1.2",
    "sass-loader": "^8.0.2",
    "script-ext-html-webpack-plugin": "^2.1.4",
    "style-loader": "^1.1.3",
    "terser-webpack-plugin": "^2.3.5",
    "ts-jest": "^25.3.1",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3",
    "url-loader": "^4.0.0",
    "webpack": "^4.42.1",
